Drop TABLE CaseHead;

CREATE TABLE CaseHead(
	CaseNumber  INTEGER PRIMARY KEY,
	PatientName TEXT Not Null,
	ChiefComplaint TEXT NOT NULL,
	ScenarioDeveloper TEXT NOT NULL,
    LastRevDate TEXT,
	DateDeveloped TEXT,
	Version REAL,
	Affiliations TEXT NOT NULL,
	ContactEmail TEXT,
	Revisedby1 TEXT,
	Revisedby2 TEXT,
	Age TEXT,
	Gender TEXT,
	Weight TEXT,
	History1 TEXT,
	History2 TEXT,
	CurrentMedications1 TEXT,
	CurrentMedications2 TEXT,
	CurrentMedications3 TEXT,
	ROSGeneral TEXT,
	ROSHEENT TEXT,
	ROSRespiratory TEXT,
	ROSMusculoskeletal TEXT,
	ROSGastrointestinal TEXT,
	ROSGenitourinary TEXT,
	ROSSkin TEXT,
	ROSNeurologic TEXT,
	ROSBehavioral TEXT,
	PEGeneralAppearance,
	PEHEENT,
	PENeck,
	PECardiovascular TEXT,
	PERespiratory TEXT,
	PEMusculoskeletal TEXT,
	PEGastrointestinal TEXT,
	PEGenitourinary TEXT,
	PESkin TEXT,
	PENeurologic TEXT,
	PEBehavioral TEXT,
	VSTemperature REAL,
	VSSBP REAL,
	VSDBP REAL,
	VSPulse REAL,
	VSRespiratoryRate REAL,
	VSO2Sat REAL,
	FinalDiagnosis TEXT,
	Labs1 TEXT,
	Labs2 TEXT, 
	Labs3 TEXT,
	Labs4 TEXT,
	Labs5 TEXT
);

