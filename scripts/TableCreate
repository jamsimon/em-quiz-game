CREATE TABLE CaseHead(
	CaseNumber  INTEGER PRIMARY KEY,
	PatientName TEXT NOT NULL,
	ChiefComplaint TEXT NOT NULL,
	ScenarioDeveloper TEXT NOT NULL,
	LastRevDate TEXT,
	DateDeveloped TEXT,
	Version REAL,
	Affiliations TEXT NOT NULL,
	ContactEmail TEXT,
	Revisedby1 TEXT,
	Revisedby2 TEXT,
	Age TEXT,
	Gender TEXT,
	Weight TEXT,
	Temperature TEXT,
	History1 TEXT,
	History2 TEXT,
	CurrentMedications1 TEXT,
	CurrentMedications2 TEXT,
	CurrentMedications3 TEXT,
	ROSGeneral TEXT,
	ROSHEENT TEXT,
	ROSCardivascular TEXT,
	ROSRespiratory TEXT,
	ROSMusculoskeletal TEXT,
	ROSGastrointestinal TEXT,
	ROSGenitourinary TEXT,
	ROSSkin TEXT,
	ROSHemeLymph TEXT,
	ROSNeurologic TEXT,
	ROSBehavioral TEXT,
	PEAirway TEXT,
	PEBreathing TEXT,
	PECirculation TEXT,
	PEDisability TEXT,
	PEEnvironment TEXT,
	PEHeadNoseEarsThroat TEXT,
	PEEyes TEXT,
	PECardiovascular TEXT,
	PERespiratory TEXT,
	PEGastrointestinal TEXT,
	PEGenitourinary TEXT,
	PESkin TEXT,
	PENeurologic TEXT,
	PEBehavioral TEXT,
	
);



	
	